# Relayer Configuration
# Copy this file to .env and fill in the values

# REQUIRED: Private key of the relayer wallet (must have ETH for gas)
# Generate a new wallet for this - DO NOT use your personal wallet
RELAYER_PRIVATE_KEY=0x...

# REQUIRED: TangleMigration contract address
MIGRATION_CONTRACT=0x...

# RPC endpoint
RPC_URL=http://localhost:8545

# Chain ID (31337 for local, 84532 for Base Sepolia, 8453 for Base)
CHAIN_ID=31337

# Server port
PORT=3001
